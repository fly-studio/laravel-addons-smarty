{
    "name": "addons/smarty",
    "description": "This package lets you run Smarty4 on Laravel elegantly.",
    "homepage": "https://www.load-page.com/",
    "keywords": ["Laravel", "Smarty", "Smarty 4", "View", "Templating"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/fly-studio/laravel-addons-smarty/issues",
        "source": "https://github.com/fly-studio/laravel-addons-smarty"
    },
    "authors": [
        {
            "name": "Latrell Chan",
            "email": "i@latrell.me"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/support": ">=8.0",
        "illuminate/cache": ">=8.0",
        "illuminate/config": ">=8.0",
        "smarty/smarty": "4.*@stable"
    },
    "autoload": {
        "psr-4": {
            "Addons\\Smarty\\": "src/"
        },
        "classmap": [
            "/src"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addons\\Smarty\\ServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev"
}
